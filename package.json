    "eslint": "^9.26.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "jest": {
    "preset": "ts-jest",
